{
	"type":"object",
	"title":"AdForm DMP extractor",
	"properties":{
		"dateFrom":{
			"type":"string",
			"title":"date from",
			"default":"2017-12-01",
			"minLength":10,
			"description":"Date in format YYYY-MM-DD",
			"propertyOrder":1000
				},
		"dateTo":{
			"type":"string",
			"title":"date to",
			"default":"2018-12-31",
			"minLength":10,
			"description":"Date in format YYYY-MM-DD",
			"propertyOrder":2000
			},
		"groupBy":{      
      			"type": "string",
      			"enum": [
        			"agency",
        			"advertiser",
        			"segment",
        			"date",
        			"country",
        			"domain",
        			"dataProvider",
       				"none",
        			"source",
        			"contract",
        			"campaign",
        			"campaignCurrency",
        			"order",
        			"lineItem"
     		 ],
       			"default": "date",
        		"propertyOrder":4000
		},
		"bucket": {
			"propertyOrder": 4500,
			"title": "Output bucket",
			"type": "string",
			"description": "Result Storage bucket path.(example: in.c-main)"
		},
		"user":{
			"type":"string",
			"description":"AdForm user name",
			"propertyOrder":5000
			},
		"#pass":{
			"type":"string",
			"description":"AdForm password",
			"propertyOrder":6000
			}
	},
	"required": [
       		"dateFrom","dateTo", "groupBy","bucket","user","#pass" 
	]
}
	
